{
  "name": "upmeter-webui",
  "version": "1.0.0",
  "description": "Web interface for upmeter",
  "repository": "github.com/deckhouse/deckhouse",
  "license": "Apache 2.0",
  "scripts": {
    "test": "echo no tests && exit 1",
    "start:dev": "./node_modules/.bin/webpack-dev-server --hot --inline --progress",
    "build": "./node_modules/.bin/webpack --config webpack.prod.config.js",
    "analyze": "mkdir -p dist && webpack --profile --json --config webpack.prod.config.js > dist/stats.json && npx webpack-bundle-analyzer dist/stats.json dist/",
    "analyze:dev": "mkdir -p dist && webpack --profile --json > dist/stats.json && npx webpack-bundle-analyzer dist/stats.json dist/",
    "lint": "eslint --fix src",
    "fmt": "prettier --write src/**"
  },
  "keywords": [
    "upmeter"
  ],
  "author": "JSC Flant",
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "7.12.1",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "app-manifest-webpack-plugin": "^1.2.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.1.2",
    "prettier": "^2.2.1",
    "purgecss-webpack-plugin": "^3.0.0",
    "style-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "bootstrap": "4.5.3"
  }
}
